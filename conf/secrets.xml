<!--
  Do NOT comment out variables in this file as it will have no affect. The variables will still be set
-->

<!-- event_socket -->
<X-PRE-PROCESS cmd="set" data="event_socket_password=ClueCon"/>

<!-- IP Addresses -->
<X-PRE-PROCESS cmd="set" data="external_ip=auto-nat"/>

<!-- Codec Preferences -->
<X-PRE-PROCESS cmd="set" data="global_codec_prefs=G722,PCMU,PCMA,GSM,G729"/>

<!-- Rayo -->
<X-PRE-PROCESS cmd="set" data="rayo_domain_name=localhost"/>
<X-PRE-PROCESS cmd="set" data="rayo_shared_secret=secret"/>
<X-PRE-PROCESS cmd="set" data="rayo_ip=0.0.0.0"/>
<X-PRE-PROCESS cmd="set" data="rayo_port=5222"/>
<X-PRE-PROCESS cmd="set" data="rayo_user=adhearsion"/>
<X-PRE-PROCESS cmd="set" data="rayo_password=secret"/>

<!-- DTMF Settings -->
<X-PRE-PROCESS cmd="set" data="start_inband_dtmf_detection=true"/>
